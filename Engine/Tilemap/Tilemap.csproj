    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageId>Pure-Tilemap</PackageId>
    <Title>Pure-Tilemap</Title>
    <Authors>gojur</Authors>
    <RepositoryUrl>https://github.com/georgi-kanchev/Pure</RepositoryUrl>
